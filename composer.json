{
    "name":        "jiabin/julius-referer-bundle",
    "type":        "symfony-bundle",
    "description": "Julius referer manager",
    "keywords":    ["julius", "referer", "http"],
    "homepage":    "https://github.com/jiabin/JuliusRefererBundle",
    "license":     "MIT",
    "authors": [
        {
            "name": "Eymen Gunay",
            "email": "eymen@egunay.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.1,<2.5-dev",
        "doctrine/common": ">=2.1.3",
        "stof/doctrine-extensions-bundle": ">=1.1.x-dev"
    },
    "suggest": {
        "cedriclombardot/admingenerator-generator-bundle": "dev-master",
        "doctrine/mongodb-odm-bundle": ">=2.1.0"
    },
    "target-dir": "Julius/RefererBundle",
    "autoload": {
        "psr-0": {
            "Julius\\RefererBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
